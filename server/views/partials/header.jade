        li
          a(href='./watch.html') Watch
        li
          a(href='./infos.html') Infos
      .sociaux
        img(src='css/img/facebook.png', alt='')
        img(src='css/img/twitter.png', alt='')
        img(src='css/img/googlep.png', alt='')